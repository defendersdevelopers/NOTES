<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/imj"
    tools:context="com.defenders.notes.MathsActivity">
<androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <com.google.android.gms.ads.AdView
            xmlns:ads="http://schemas.android.com/apk/res-auto"
            android:id="@+id/adView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            ads:adSize="BANNER"
            android:layout_gravity="center"
            ads:adUnitId="@string/banner4"/>
        <Button
            android:id="@+id/button01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Real Numbers" />

        <Button
            android:id="@+id/button02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Polynomials" />


        <Button
            android:id="@+id/button03"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Quadratic Equation" />

        <Button
            android:id="@+id/button04"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Pair of linear Equations in Two Variable" />

        <Button
            android:id="@+id/button05"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Arithmetic Progressions" />

        <Button
            android:id="@+id/button06"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Triangle" />


        <Button
            android:id="@+id/button07"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Coordinate Geometry" />


        <Button
            android:id="@+id/button08"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Introduction to Trigonometry" />

        <Button
            android:id="@+id/button09"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Some Applications of Trigonometry" />

        <Button
            android:id="@+id/button010"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Area Related to Circle" />

        <Button
            android:id="@+id/button011"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Surface Area and Volumes" />

        <Button
            android:id="@+id/button012"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Probability" />


        <Button
            android:id="@+id/button013"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="Sample Paper" />

        <com.google.android.gms.ads.AdView
            xmlns:ads="http://schemas.android.com/apk/res-auto"
            android:id="@+id/adView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            ads:adSize="BANNER"
            android:layout_gravity="center"
            ads:adUnitId="@string/banner5"/>
    </LinearLayout>


</androidx.core.widget.NestedScrollView>
</RelativeLayout>